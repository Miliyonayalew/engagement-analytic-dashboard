{
  "name": "engagement-analytics-dashboard",
  "version": "1.0.0",
  "description": "Interactive engagement analytics dashboard with backend and frontend",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run backend\" \"npm run frontend\"",
    "backend": "cd ../backend && npm run dev",
    "frontend": "wait-on http://localhost:8000 && vite --host",
    "start": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo 'âœ… Tests would run here - currently using build as validation'",
    "test:type-check": "tsc --noEmit --skipLibCheck",
    "test:build": "npm run build",
    "backend-only": "cd ../backend && npm run dev",
    "frontend-only": "vite --host",
    "serve-static": "cd public && python3 -m http.server 8000"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.0.5",
    "wait-on": "^7.2.0"
  },
  "keywords": [
    "engagement",
    "analytics",
    "dashboard",
    "interactive",
    "react",
    "vite"
  ],
  "author": "Your Name",
  "license": "MIT"
}
